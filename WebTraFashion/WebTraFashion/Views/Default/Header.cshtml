            <div class="toggle-search"></div>
            <div class="toggle_cont">
                <div id="search">
                    <input type="text" name="search" value="" placeholder="Search..." />
                    <button type="button" class="button-search"></button>
                    <i class="closebtn"></i>
                </div>
            </div>
        </div>
    </div>
